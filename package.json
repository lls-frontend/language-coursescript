{
  "name": "language-coursescript",
  "version": "3.54.1",
  "description": "Syntax highlighting and snippets for CourseScript.",
  "repository": "https://github.com/lls-frontend/language-coursescript.git",
  "license": "MIT",
  "engines": {
    "atom": "*"
  },
  "activationCommands": {
    "atom-text-editor": [
      "language-coursescript:insert-darwin-id",
      "language-coursescript:insert-cc-id",
      "language-coursescript:insert-bell-id",
      "language-coursescript:preview",
      "language-coursescript:search"
    ],
    "atom-workspace": [
      "language-coursescript:focus-search"
    ]
  },
  "devDependencies": {
    "coffee-script": "1.7.0",
    "coffeelint": "^1.10.1",
    "request": "^2.34.0",
    "season": "^5.0.2",
    "underscore-plus": "^1.0.6"
  },
  "main": "./lib/language-coursescript",
  "dependencies": {
    "@laix/coursescript-lib": "^2.33.2",
    "@laix/coursescript-lib-legacy": "^1.0.0",
    "@laix/coursescript-lib-sprout": "^2.45.0",
    "atom-space-pen-views": "^2.2.0",
    "axios": "^0.18.0",
    "dayjs": "^1.6.7",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
